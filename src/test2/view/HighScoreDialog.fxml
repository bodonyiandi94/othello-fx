<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="test2.controller.HighScoreDialogController">
  <children>
    <TableView fx:id="tableView" editable="true" layoutX="0.0" layoutY="0.0" prefHeight="357.0" prefWidth="600.0">
      <columns>
        <TableColumn editable="false" prefWidth="150.0" text="Name" fx:id="nameColumn" />
        <TableColumn editable="false" prefWidth="150.0" text="Wins" fx:id="winsColumn" />
        <TableColumn editable="false" prefWidth="150.0" text="Losses" fx:id="lossesColumn" />
        <TableColumn editable="false" prefWidth="150.0" resizable="true" text="Best score" fx:id="bestScoreColumn" />
      </columns>
    </TableView>
    <Button fx:id="okButton" layoutX="272.0" layoutY="366.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Ok" />
  </children>
</AnchorPane>
